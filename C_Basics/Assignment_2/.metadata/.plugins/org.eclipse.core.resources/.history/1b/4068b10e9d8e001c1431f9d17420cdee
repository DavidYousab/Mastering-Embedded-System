#include "stdio.h"

void main()
{
	char A; float num1,num1;
	printf("Enter an operator(+,-,*,/)");
	fflush(stdin); fflush(stdout);
	scanf("%c",&A);

	printf("Enter 2 numbers");
	fflush(stdin); fflush(stdout);
	scanf("%d %d",&num1,&num2);

	switch(A)
	{
	case '+':

		printf("%c is Vowel",A);
	break;

	case '-':
		printf("%c is Vowel",A);
	break;


	case '*':
		printf("%c is Vowel",A);
	break;

	case '/':
		printf("%c is Vowel",A);
	break;
	default:
		printf("%c is constant",A);
		break;
	}

}
