#include "stdio.h"

void main()
{
	char A; float num1,num2;
	printf("Enter an operator(+,-,*,/)");
	fflush(stdin); fflush(stdout);
	scanf("%c",&A);

	printf("Enter 2 numbers");
	fflush(stdin); fflush(stdout);
	scanf("%d %d",&num1,&num2);

	switch(A)
	{
	case '+':

		printf("%d",num1+num2);

	break;

	case '-':
		printf("%d",num1-num2);
	break;


	case '*':
		printf("%d",num1*num2);
	break;

	case '/':
		printf("%d",num1/num2);
	break;
	default:
		printf("No Match ");
		break;
	}

}
